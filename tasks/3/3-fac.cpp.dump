#include <iostream>

int main() {
    int x;
    for (int i=0; i<5; i++) {
        __uint128_t fac = 1;
        for (unsigned int n=3*(i+1)*(i+1); n>=0; n=n-2)
        {
            if (n==0 || n==1)
                {std::cout << 3*(i+1)*(i+1) << " ";
                break;}
            else
            {fac *= n; std::cout << fac << std::endl;}
        }
        std::cout << fac << std::endl;
    }
    std::cin >> x;
    return 0;
}